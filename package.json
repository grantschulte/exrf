{
  "name": "exrf",
  "version": "0.1.0",
  "description": "Express, React, Flow, and Webpack Starter",
  "main": "server.js",
  "scripts": {
    "build": "yarn clean && NODE_ENV=production webpack",
    "clean": "rm -rf ./client/build",
    "client:dev": "webpack-dev-server",
    "flow:client": "yarn run flow status",
    "lint:client": "eslint ./client/**/*.js",
    "lint:server": "eslint ./server/**/*.js",
    "server:dev": "nodemon server.js",
    "start": "node server.js",
    "test:server": "NODE_ENV=test mocha ./client/src/**/*.spec.js --exit",
    "test:server": "NODE_ENV=test mocha ./server/**/*.spec.js --exit"
